<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.main.MainFragment">

    <Button
        android:id="@+id/btnSaveTest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TestSave"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.393"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <AutoCompleteTextView
        android:id="@+id/PrinterAutoComplete"
        android:layout_width="227dp"
        android:layout_height="44dp"
        android:layout_marginBottom="16dp"
        android:text="Printer"
        app:layout_constraintBottom_toTopOf="@+id/materialType"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/modelButton" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginTop="43dp"
        android:layout_marginBottom="22dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Slice File Name"
        app:layout_constraintBottom_toTopOf="@+id/modelButton"
        app:layout_constraintStart_toStartOf="@+id/modelButton"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/modelButton"
        android:layout_width="217dp"
        android:layout_height="48dp"
        android:layout_marginBottom="20dp"
        android:text="Select Model"
        app:layout_constraintBottom_toTopOf="@+id/PrinterAutoComplete"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName" />

    <RadioGroup
        android:id="@+id/materialType"
        android:layout_width="227dp"
        android:layout_height="35dp"
        android:layout_marginBottom="23dp"
        app:layout_constraintBottom_toTopOf="@+id/materialAutoComplete"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/PrinterAutoComplete" />

    <AutoCompleteTextView
        android:id="@+id/materialAutoComplete"
        android:layout_width="220dp"
        android:layout_height="54dp"
        android:layout_marginBottom="24dp"
        android:text="Material"
        app:layout_constraintBottom_toTopOf="@+id/resinBaseLayers"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/materialType" />

    <EditText
        android:id="@+id/resinBaseLayers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="29dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/resinBaseLayerCureTime"
        app:layout_constraintEnd_toEndOf="@+id/materialAutoComplete"
        app:layout_constraintTop_toBottomOf="@+id/materialAutoComplete" />

    <EditText
        android:id="@+id/numberOfLayers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="34dp"
        android:ems="10"
        android:inputType="number|numberDecimal"
        app:layout_constraintBottom_toTopOf="@+id/resinLayerThickness"
        app:layout_constraintStart_toStartOf="@+id/resinLiftHeight"
        app:layout_constraintTop_toBottomOf="@+id/resinLiftHeight" />

    <EditText
        android:id="@+id/resinBaseLayerCureTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="22dp"
        android:ems="10"
        android:inputType="number|numberDecimal"
        app:layout_constraintBottom_toTopOf="@+id/resinLiftSpeed"
        app:layout_constraintStart_toStartOf="@+id/resinLiftSpeed"
        app:layout_constraintTop_toBottomOf="@+id/resinBaseLayers" />

    <EditText
        android:id="@+id/resinLiftSpeed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="22dp"
        android:ems="10"
        android:inputType="number|numberDecimal"
        app:layout_constraintBottom_toTopOf="@+id/resinRetractSpeed"
        app:layout_constraintStart_toStartOf="@+id/resinRetractSpeed"
        app:layout_constraintTop_toBottomOf="@+id/resinBaseLayerCureTime" />

    <EditText
        android:id="@+id/resinRetractSpeed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="30dp"
        android:ems="10"
        android:inputType="number|numberDecimal"
        app:layout_constraintBottom_toTopOf="@+id/resinLiftHeight"
        app:layout_constraintEnd_toEndOf="@+id/resinLiftHeight"
        app:layout_constraintTop_toBottomOf="@+id/resinLiftSpeed" />

    <EditText
        android:id="@+id/resinLiftHeight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="38dp"
        android:ems="10"
        android:inputType="number|numberDecimal"
        app:layout_constraintBottom_toTopOf="@+id/numberOfLayers"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resinRetractSpeed" />

    <EditText
        android:id="@+id/resinLayerThickness"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="37dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toTopOf="@+id/resinLayerCureTime"
        app:layout_constraintStart_toStartOf="@+id/numberOfLayers"
        app:layout_constraintTop_toBottomOf="@+id/numberOfLayers" />

    <EditText
        android:id="@+id/resinLayerCureTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="39dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toTopOf="@+id/filamentNozzleThickness"
        app:layout_constraintEnd_toEndOf="@+id/resinLayerThickness"
        app:layout_constraintTop_toBottomOf="@+id/resinLayerThickness" />

    <EditText
        android:id="@+id/filamentNozzleThickness"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="31dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toTopOf="@+id/filamentNozzleEstimatedTime"
        app:layout_constraintStart_toStartOf="@+id/resinLayerCureTime"
        app:layout_constraintTop_toBottomOf="@+id/resinLayerCureTime" />

    <EditText
        android:id="@+id/filamentNozzleEstimatedTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="39dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toTopOf="@+id/filamentEstimatedMaterial"
        app:layout_constraintStart_toStartOf="@+id/filamentNozzleThickness"
        app:layout_constraintTop_toBottomOf="@+id/filamentNozzleThickness" />

    <EditText
        android:id="@+id/filamentEstimatedMaterial"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/filamentNozzleEstimatedTime"
        app:layout_constraintTop_toBottomOf="@+id/filamentNozzleEstimatedTime" />

</androidx.constraintlayout.widget.ConstraintLayout>